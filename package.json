{
  "name": "init-next-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run lingui:extract:compile && next dev",
    "build": "npm run lingui:extract:compile && npm run lint && next build && npm run image:optimize",
    "start": "serve dist",
    "lint": "biome lint --error-on-warnings . && next lint",
    "image:optimize": "next-image-export-optimizer",
    "biome:format": "biome format --write .",
    "lingui:extract:compile": "lingui extract && lingui compile"
  },
  "dependencies": {
    "@lingui/core": "5.2.0",
    "@lingui/react": "5.2.0",
    "axios": "1.7.9",
    "classnames": "2.5.1",
    "next": "15.1.6",
    "next-image-export-optimizer": "1.18.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "swr": "2.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@lingui/cli": "5.2.0",
    "@lingui/loader": "5.2.0",
    "@lingui/swc-plugin": "5.0.2",
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.17.16",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "eslint": "9.19.0",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-lingui": "0.9.0",
    "serve": "14.2.4",
    "typescript": "5.7.3"
  },
  "overrides": {
    "@swc/core": "1.3.56"
  }
}
